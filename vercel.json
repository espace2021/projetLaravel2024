{
    "version": 2,
    "public": true,
    "builds": [
      { "src": "public/frontend/assets/*", "use": "@vercel/static" }
    ],
    "framework": null,
    "headers": [
        {
            "source": "/frontend/assets/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript"
                }
            ]
        }
    ],
    "rewrites": [
        { "source": "/frontend/assets/(.*)", "destination": "/public/frontend/assets/$1" },
        { "source": "/(.*)", "destination": "/public/index.php" }
      ]
  }
  